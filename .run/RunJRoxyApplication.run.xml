<component name="ProjectRunConfigurationManager">
    <configuration default="false" name="RunJRoxyApplication" type="Application" factoryName="Application">
        <option name="MAIN_CLASS_NAME" value="com.belhaddou.jroxy.JRoxyApplication"/>
        <module name="jproxy"/>
        <option name="VM_PARAMETERS" value="-Dspring.profiles.active=prod"/>
        <extension name="coverage">
            <pattern>
                <option name="PATTERN" value="com.belhaddou.jroxy.*"/>
                <option name="ENABLED" value="true"/>
            </pattern>
        </extension>
        <method v="2">
            <option name="Make" enabled="true"/>
        </method>
    </configuration>
</component>